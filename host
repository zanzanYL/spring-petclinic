# group name of hosts
[webservers]

# list of hosts
appserver ansible_host=172.19.0.4 ansible_user=jenkins ansible_ssh_private_key_file=/var/jenkins_home/.ssh/ansible

#docker ansible_host=172.19.0.3
#jenkins ansible_host=172.19.0.5
#sonarqube ansible_host=172.19.0.2

id_rsa ansible_connection=local

# sets variables applied to all hosts
[all:vars]
# sets Ansible to use Python3 on all managed hosts
ansible_python_interpreter=/usr/bin/python3pwd



